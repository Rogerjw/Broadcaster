{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": "10.16.3"
  },
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@hapi/joi": "^16.1.7",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "config": "^3.2.4",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "pg": "^7.14.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.8"
  },
  "scripts": {
    "createTable": "node --require @babel/polyfill --require @babel/register ./server/v2/models/createTables.models.js",
    "deleteTable": "node --require @babel/polyfill --require @babel/register ./server/v2/models/deleteTables.models.js",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register ./server/v2/tests/*.test.js --timeout 10000 --exit  ",
    "start": "babel-node --experimental-worker ./server/server.js",
    "dev": "node_modules/.bin/nodemon --experimental-worker ./server/server.js --exec babel-node --",
    "build": "rm -rf ./build && babel -d ./build ./server -s",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
